<head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />

        <!-- üî¥ IMPORTANT: Ajoutez votre code de v√©rification Google Search Console ici -->
        <!-- D√©commentez la ligne ci-dessous et remplacez YOUR_VERIFICATION_CODE par votre code -->
        <!-- <meta name="google-site-verification" content="YOUR_VERIFICATION_CODE" /> -->
        
        <!-- Bing Webmaster Tools Verification (optionnel) -->
        <!-- <meta name="msvalidate.01" content="YOUR_BING_VERIFICATION_CODE" /> -->

        <!-- SEO Meta Tags with Safe Fallbacks -->
        <title>
                <%= typeof seo !=='undefined' && seo?.title ? seo.title
                        : 'Math√©matiques Bac Marocain - Exercices et Solutions' %>
        </title>
        <meta name="description"
                content="<%= typeof seo !== 'undefined' && seo?.description ? seo.description : 'Exercices et solutions de math√©matiques pour le baccalaur√©at marocain. Cours complets et exercices corrig√©s.' %>" />
        <meta name="keywords"
                content="<%= typeof seo !== 'undefined' && seo?.keywords ? seo.keywords : 'math√©matiques, bac marocain, exercices, solutions, arithm√©tique, alg√®bre, g√©om√©trie' %>" />
        <meta name="author" content="Abdelhadi Nakhal" />
        <% if (typeof noindex !== 'undefined' && noindex === true) { %>
        <meta name="robots" content="noindex, nofollow" />
        <meta name="googlebot" content="noindex, nofollow" />
        <% } else { %>
        <meta name="robots" content="index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1" />
        <meta name="googlebot" content="index, follow" />
        <meta name="bingbot" content="index, follow" />
        <% } %>

        <!-- Canonical URL -->
        <link rel="canonical" href="<%= typeof seo !== 'undefined' && seo?.canonical ? seo.canonical : 'https://www.mathematiques-bac.org/' %>" />

        <!-- Language and Region -->
        <meta http-equiv="content-language" content="fr" />
        <!-- Removed geo.region to target global audience (Francophonie & International) -->

        <!-- Open Graph for Social Sharing -->
        <meta property="og:title"
                content="<%= typeof seo !== 'undefined' && seo?.title ? seo.title : 'Math√©matiques Bac & High School Math' %>" />
        <meta property="og:description"
                content="<%= typeof seo !== 'undefined' && seo?.description ? seo.description : 'Math√©matiques pour le baccalaur√©at et le lyc√©e. Exercices corrig√©s, cours et solutions. Mathematics exercises and solutions for High School.' %>" />
        <meta property="og:type" content="website" />
        <meta property="og:url" content="<%= typeof seo !== 'undefined' && seo?.canonical ? seo.canonical : 'https://www.mathematiques-bac.org' %>" />
        <meta property="og:image" content="<%= typeof seo !== 'undefined' && seo?.ogImage ? seo.ogImage : 'https://www.mathematiques-bac.org/assets/img/home-bg.jpg' %>" />
        <meta property="og:image:width" content="1200" />
        <meta property="og:image:height" content="630" />
        <meta property="og:site_name" content="Math√©matiques Bac & High School Math" />
        <meta property="og:locale" content="fr_FR" />
        <meta property="og:locale:alternate" content="en_US" />

        <!-- Twitter Card -->
        <meta name="twitter:card" content="summary_large_image" />
        <meta name="twitter:title" content="<%= typeof seo !== 'undefined' && seo?.title ? seo.title : 'Math√©matiques Bac & High School Math' %>" />
        <meta name="twitter:description" content="<%= typeof seo !== 'undefined' && seo?.description ? seo.description : 'Exercices et solutions de math√©matiques pour le lyc√©e et le baccalaur√©at. High School Math Exercises.' %>" />
        <meta name="twitter:image" content="<%= typeof seo !== 'undefined' && seo?.ogImage ? seo.ogImage : 'https://www.mathematiques-bac.org/assets/img/home-bg.jpg' %>" />

        <!-- Structured Data - Educational Website -->
        <script type="application/ld+json">
        {
                "@context": "https://schema.org",
                "@type": "EducationalWebsite",
                "name": "Math√©matiques Bac & High School Math",
                "description": "Plateforme d'exercices corrig√©s e de cours de math√©matiques pour le lyc√©e et le baccalaur√©at (Terminale). International High School Mathematics.",
                "url": "https://www.mathematiques-bac.org",
                "inLanguage": ["fr", "en"],
                "publisher": {
                        "@type": "Organization",
                        "name": "Math√©matiques Bac Marocain",
                        "url": "https://www.mathematiques-bac.org"
                },
                "educationalLevel": "SecondaryEducation",
                "audience": {
                        "@type": "EducationalAudience",
                        "educationalRole": "student"
                },
                "about": {
                        "@type": "Thing",
                        "name": "Mathematics Education"
                }
        }
        </script>
        <link rel="icon" type="image/x-icon" href="/assets/favicon.ico" />
        <!-- Font Awesome icons (CSS version for better CSP compatibility)-->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
                integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw=="
                crossorigin="anonymous" referrerpolicy="no-referrer" />
        <!-- Google fonts - Updated API with CSP-friendly preconnect -->
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Lora:ital,wght@0,400;0,700;1,400;1,700&family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;0,800;1,300;1,400;1,600;1,700;1,800&display=swap"
                rel="stylesheet">
        <!-- Core theme CSS (includes Bootstrap)-->
        <link href="/css/styles.css?v=2.0" rel="stylesheet" />
        <!-- MathJax (TeX rendering) - Alternative CDN to bypass cache -->
        <script>
                window.MathJax = {
                        tex: {
                                inlineMath: [['$', '$'], ['\\(', '\\)']],
                                displayMath: [['$$', '$$'], ['\\[', '\\]']],
                                processEscapes: true,
                                processEnvironments: true
                        },
                        options: {
                                skipHtmlTags: ['script', 'noscript', 'style', 'textarea', 'pre']
                        },
                        startup: {
                                pageReady: () => {
                                        return MathJax.startup.defaultPageReady().then(() => {
                                                console.log('‚úÖ MathJax 3.2.2 loaded successfully');
                                        });
                                }
                        }
                };
                window.addEventListener('error', (e) => {
                        if (e.filename && e.filename.includes('mathjax')) {
                                console.error('‚ùå MathJax loading error:', e.message);
                        }
                });
        </script>
        <script src="https://cdn.jsdelivr.net/npm/mathjax@3.2.2/es5/tex-chtml.js" id="MathJax-script" async></script>
        <% if (isProduction && umami && umami.src && umami.websiteId) { %>
        <script defer src="<%= umami.src %>" data-website-id="<%= umami.websiteId %>"></script>
        <% } else { %>
        <!-- Umami not loaded: set UMAMI_SRC and UMAMI_WEBSITE_ID in Railway for production tracking -->
        <% } %>
</head>
